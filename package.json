    "arco-design-pro-vue": "^2.6.0",
    "axios": "^1.2.2",
    "clipboard": "^2.0.0",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "js-md5": "^0.7.3",
    "lodash": "4.17.15",
    "md-editor-v3": "^2.7.2",
    "mitt": "^3.0.0",
    "monaco-editor": "^0.31.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "query-string": "^8.0.3",
    "sm-crypto": "^0.3.12",
    "sortablejs": "^1.15.0",
    "sso-login-logic": "file:../sso-login-logic",
    "tinymce": "^6.3.1",
    "vue": "^3.3.7",
    "vue-echarts": "^6.2.3",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.14",
    "vue3-print-nb": "^0.1.4"
