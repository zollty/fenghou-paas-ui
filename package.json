{
  "name": "param-backstage-frame",
  "description": "数地中后台架子",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": "XK",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts --mode development",
    "dev:component": "vite --config ./config/vite.config.dev.ts --mode devcomponent",
    "build:sit": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts --mode sit",
    "create-package": "node ./config/node/create-package.js",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts --mode production",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview --host",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,build}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix",
    "lint:ts": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*src/**/*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix"
    ],
    "*packages/**/*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix"
    ],
    "*src/**/*.{vue,sass,scss,less}": [
      "eslint --fix"
    ],
    "*packages/**/*.{vue,sass,scss,less}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
